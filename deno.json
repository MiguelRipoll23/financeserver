{
  "imports": {
    "@ai-sdk/openai": "npm:@ai-sdk/openai@^3.0.26",
    "ai": "npm:ai@^6.0.77",
    "@hono/zod-openapi": "npm:@hono/zod-openapi@^1.2.1",
    "@simplewebauthn/server": "jsr:@simplewebauthn/server@^13.2.2",
    "@needle-di/core": "jsr:@needle-di/core@^1.1.0",
    "@scalar/hono-api-reference": "npm:@scalar/hono-api-reference@^0.9.40",
    "@hono/mcp": "npm:@hono/mcp@0.2.3",
    "@modelcontextprotocol/sdk/server/mcp.js": "npm:@modelcontextprotocol/sdk@^1.26.0/server/mcp.js",
    "@wok/djwt": "jsr:@wok/djwt@^3.0.2",
    "drizzle-kit": "npm:drizzle-kit@^0.31.8",
    "drizzle-orm": "npm:drizzle-orm@^0.45.1",
    "pg": "npm:pg@^8.18.0",
    "hono": "npm:hono@^4.11.8",
    "zod": "npm:zod@^4.3.6"
  },
  "unstable": [
    "kv"
  ],
  "permissions": {
    "development": {
      "env": true,
      "read": true,
      "net": true
    }
  },
  "tasks": {
    "dev": "deno run --unstable-kv --permission-set=development --env --watch src/main.ts",
    "debug": "deno run --unstable-kv --inspect-wait --permission-set=development --env --watch src/main.ts",
    "check": "deno check --unstable-kv src/main.ts",
    "studio": "dx npm:drizzle-kit studio",
    "generate": "dx npm:drizzle-kit generate",
    "migrate": "dx npm:drizzle-kit migrate"
  },
  "deploy": {
    "predeploy": "deno task migrate",
    "runtime": {
      "entrypoint": "src/main.ts",
      "args": ["--unstable-kv"]
    }
  }
}
